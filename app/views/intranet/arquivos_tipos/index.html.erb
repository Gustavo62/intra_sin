<p id="notice"><%= notice %></p>

<h1>Intranet Arquivos Tipos</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th>Ativo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intranet_arquivos_tipos.each do |intranet_arquivos_tipo| %>
      <tr>
        <td><%= intranet_arquivos_tipo.descricao %></td>
        <td><%= intranet_arquivos_tipo.ativo %></td>
        <td><%= link_to 'Show', intranet_arquivos_tipo %></td>
        <td><%= link_to 'Edit', edit_intranet_arquivos_tipo_path(intranet_arquivos_tipo) %></td>
        <td><%= link_to 'Destroy', intranet_arquivos_tipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

