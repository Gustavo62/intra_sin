<div class="container">
    <div class="top-dash">
        <div class="moldura">
            <div class="box um">
                <%= icon('person-fill', class:"icon-dash")%> 
                <p style="text-align: center;">

                    <b style="font-size: 28px;color: white;">
                        <%= @cartorio_cad %>
                    </b>  
                    
                    <h6 style="text-align: center;">Cartórios Cadastrados</h6>
                </p> 
            </div>
        </div>

        <div class="moldura">
            <div class="box dois">
                <%= icon('person-check-fill', class:"icon-dash")%> 
                <p style="text-align: center;text-align: center;"> 
                    <b style="font-size: 28px;color: white;">
                        <%= @cartorio_cad_c_assoc %>
                    </b>  
                    
                    <h6 style="text-align: center;width: 126px;margin-left: -16px;">Cartórios<br>com Associado</h6>
                </p> 
            </div>
        </div>

        <div class="moldura">
            <div class="box tres">
                <%= icon('cash-stack', class:"icon-dash")%> 
                <p style="text-align: center;"> 
                    
                    <b style="font-size: 28px;color: white;">
                        <%= @cart_adiplente %>
                    </b>  
                    
                    <h6 style="text-align: center;">Cartórios Adiplentes</h6>
                </p> 
            </div>
        </div>

        <div class="moldura">
            <div class="box quatro">
                <%= icon('award', class:"icon-dash")%> 
                <p style="text-align: center;"> 
                    <b style="font-size: 28px;color: white;">
                        <%= @cartorio_cad_c_assoc_ativo %>
                    </b>  
                    
                    <h6 style="text-align: center;">Associados Ativos</h6>
                </p> 
            </div> 
        </div>
    </div> 
</div>
<style> 
.moldura{ 
    margin-top: 20px;
    background: white;
    width: 145px;
    height: 209px;
    box-shadow: 0px 1px 6px 3px #e6e6e6;
    margin-left: auto;
    margin-right: auto;
}
.top-dash{
    display:flex;
    flex-flow: wrap;
    padding-top:20px
}
.box{
    padding:2px
}
.um, .dois, .tres, .quatro{
    width: 100px;
    height: 121px;
    margin: 21px;
    margin-left:auto;
    margin-right:auto;
}
.um{
background-color: #00c0ef;
}
.dois{
background-color: #dd4b39;
}
.tres{
background-color: #00a65a;
}
.quatro{
background-color: #f39c12;
}
.icon-dash{
    margin: 26px;
    padding: unset;
    width: 46px;
    height: 49px;
    color: #fff;
    margin-bottom: 0px;
}
</style>